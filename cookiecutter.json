{
  "project_name": "Project folder boilerplate",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
  "project_name_dashed": "{{ cookiecutter.project_slug|replace('_', '-') }}",
  "project_name_dashed_timestamped": "{% now 'utc', '%Y-%m-%d' %}-{{ cookiecutter.project_name_dashed }}",
  "_extensions": ["jinja2_time.TimeExtension"]
}
